{
  "name": "@lyvely/pwa",
  "version": "0.1.0-alpha3",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "lyvely-dev build:web",
    "lint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier src --write",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "deps": "pnpm depcruise --include-only \"^src\" --config .dependency-cruiser.js src",
    "ncu": "ncu",
    "build:staging": "vite build --mode staging && vue-tsc --emitDeclarationOnly",
    "build:dev": "vite build --mode development && vue-tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@lyvely/web": "workspace:*",
    "@lyvely/ui": "workspace:*",
    "@lyvely/activities-web": "workspace:*",
    "@lyvely/calendar-plan-web": "workspace:*",
    "@lyvely/time-series-web": "workspace:*",
    "@lyvely/analytics-web": "workspace:*",
    "@lyvely/habits-web": "workspace:*",
    "@lyvely/journals-web": "workspace:*",
    "@lyvely/legal-web": "workspace:*",
    "@lyvely/milestones-web": "workspace:*",
    "@lyvely/tasks-web": "workspace:*",
    "vue": "^3.4",
    "workbox-core": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-window": "^7.1.0",
    "workbox-build": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "reflect-metadata": "^0.1.12 || ^0.2.0"
  },
  "devDependencies": {
    "@lyvely/devtools": "workspace:*",
    "vue-router": "^4.3",
    "vite": "^5.2",
    "vite-plugin-pwa": "^0.20.0",
    "@vue/runtime-dom": "^3.4",
    "vue-tsc": "^2.0",
    "postcss": "^8.4",
    "postcss-cli": "^11.0",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "eslint-plugin-vue": "^9.25",
    "prettier-plugin-tailwindcss": "^0.5",
    "vue-eslint-parser": "^9.4",
    "depcheck": "^1.4",
    "dependency-cruiser": "^14.1"
  }
}
