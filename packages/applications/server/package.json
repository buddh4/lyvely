{
  "name": "@lyvely/server",
  "type": "commonjs",
  "version": "0.1.0-alpha3",
  "description": "",
  "author": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "prettier \"src/**/*.ts\" --write",
    "prettier": "prettier . --check",
    "dev": "cross-env NODE_ENV=development nest start",
    "dev:debug": "cross-env NODE_ENV=development  nest start --debug --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "start:debug": "nest start --debug --watch"
  },
  "dependencies": {
    "@lyvely/api": "workspace:*",
    "@lyvely/habits": "workspace:*",
    "@lyvely/tasks": "workspace:*",
    "@lyvely/journals": "workspace:*",
    "@lyvely/analytics": "workspace:*",
    "@lyvely/milestones": "workspace:*",
    "@lyvely/legal": "workspace:*",
    "@lyvely/interface": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "tslib": "^2.6.2",
    "tsc-alias": "^1.8.10",
    "rimraf": "^5.0.5"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "5.0.9"
    }
  }
}
