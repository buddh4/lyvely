{
  "name": "@lyvely/server",
  "type": "commonjs",
  "version": "0.1.1",
  "author": "Julian Harrer",
  "license": "AGPL-3.0-or-later",
  "description": "A Lyvely server application including all core and main platform features.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "prettier src --write",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "deps": "rush-pnpm --rush-skip-checks depcruise --include-only \"^src\" --config .dependency-cruiser.js src",
    "ncu": "ncu",
    "dev": "cross-env NODE_ENV=development nest start",
    "dev:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "start:debug": "nest start --debug --watch"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "tslib": "^2.6.2",
    "@lyvely/api": "workspace:*",
    "@lyvely/habits": "workspace:*",
    "@lyvely/tasks": "workspace:*",
    "@lyvely/journals": "workspace:*",
    "@lyvely/analytics": "workspace:*",
    "@lyvely/milestones": "workspace:*",
    "@lyvely/legal": "workspace:*"
  },
  "devDependencies": {
    "@lyvely/devtools": "workspace:*",
    "typescript": "^5.4",
    "tsc-alias": "^1.8.10",
    "rimraf": "^5.0",
    "depcheck": "^1.4",
    "dependency-cruiser": "^14.1",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "cross-env": "^7.0"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "5.0.9"
    }
  }
}
