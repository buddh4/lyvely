    "vue-tsc": "^2.0.14",
    "tailwind": "^4.0.0",
    "postcss": "^8.4.23",
    "autoprefixer": "^10.4.14",
    "@vue/runtime-dom": "^3.4.25",
    "vue-router": "^4.3.2"
