{
  "name": "@lyvely/habits-interface",
  "version": "0.1.1",
  "author": "Julian Harrer",
  "license": "AGPL-3.0-or-later",
  "description": "Lyvely habits interface",
  "type": "commonjs",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "lint": "eslint --fix",
    "format": "prettier src --write",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "deps": "rush-pnpm --rush-skip-checks depcruise --include-only \"^src\" --config .dependency-cruiser.js src",
    "ncu": "ncu",
    "build": "rimraf dist && tsc -p tsconfig.build.json --outDir dist/lib --declarationDir dist/lib --declaration true && tscpaths -p tsconfig.build.json -s ./src -o dist/lib && rollup -c --silent && rimraf tsconfig.tsbuildinfo",
    "prepublish:npm": "rushx build",
    "prepublish:next": "pnpm run build",
    "prerelease": "rushx build",
    "test": "jest --passWithNoTests",
    "clear": "rimraf dist && rimraf./tsconfig.tsbuildinfo",
    "test:cov": "jest --coverage --no-cache"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "tslib": "^2.6.2",
    "@lyvely/dates": "workspace:*",
    "@lyvely/common": "workspace:*",
    "@lyvely/interface": "workspace:*",
    "@lyvely/time-series-interface": "workspace:*",
    "@lyvely/calendar-plan-interface": "workspace:*",
    "@buddh4/mapped-types": "^1.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.12 || ^0.2.0"
  },
  "devDependencies": {
    "@lyvely/devtools": "workspace:*",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.4",
    "rollup": "^4.18",
    "typescript": "^5.4",
    "tscpaths": "^0.0.9",
    "@rollup/plugin-typescript": "^11.1",
    "@rollup/plugin-commonjs": "^25.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rollup/plugin-node-resolve": "^15.0",
    "rollup-plugin-node-externals": "^6.1",
    "axios": "^1.7",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "depcheck": "^1.4",
    "dependency-cruiser": "^14.1",
    "rimraf": "^5.0"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
