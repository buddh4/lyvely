{
  "name": "@lyvely/legal-web",
  "version": "0.1.0-alpha3",
  "description": "Lyvely legal web package",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build && vue-tsc -p tsconfig.build.json -declaration --emitDeclarationOnly",
    "build:staging": "vite build --mode staging && vue-tsc --emitDeclarationOnly",
    "build:dev": "vite build --mode development && vue-tsc --emitDeclarationOnly",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix",
    "format": "prettier .  --write"
  },
  "author": "Julian Harrer",
  "dependencies": {
    "vue": "^3.3.4",
    "pinia": "^2.0.35",
    "@lyvely/web": "^0.1.0-alpha3",
    "@lyvely/ui": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/legal-interface": "^0.1.0-alpha3"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@lyvely/configs": "^0.1.0-alpha3",
    "vite-plugin-externalize-deps": "^0.7.0",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.14.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vue-tsc": "^1.6.5"
  }
}
