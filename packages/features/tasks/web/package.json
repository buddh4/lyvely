{
  "name": "@lyvely/tasks-web",
  "version": "0.1.0-alpha3",
  "description": "Lyvely Tasks Web Package",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc -p tsconfig.build.json -declaration --emitDeclarationOnly",
    "build:staging": "vite build --mode staging && vue-tsc --emitDeclarationOnly",
    "build:dev": "vite build --mode development && vue-tsc --emitDeclarationOnly",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix",
    "format": "prettier .  --write"
  },
  "author": "Julian Harrer",
  "dependencies": {
    "@lyvely/calendar-plan-web": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/dates": "^0.1.0-alpha3",
    "@lyvely/tasks-interface": "^0.1.0-alpha3",
    "@lyvely/timers-interface": "^0.1.0-alpha3",
    "@lyvely/ui": "^0.1.0-alpha3",
    "@lyvely/web": "^0.1.0-alpha3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "pinia": "^2.0.35",
    "vue": "^3.3.4",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@lyvely/configs": "^0.1.0-alpha3",
    "postcss": "^8.4.31",
    "vite": "^4.3.9",
    "vite-plugin-externalize-deps": "^0.7.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vue-tsc": "^1.6.5"
  }
}
