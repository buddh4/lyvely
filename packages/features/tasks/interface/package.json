{
  "name": "@lyvely/tasks-interface",
  "version": "0.1.0-alpha3",
  "description": "Lyvely tasks interface",
  "type": "commonjs",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "lint": "eslint --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "tsc -p tsconfig.build.json --outDir dist/lib --declarationDir dist/lib --declaration true && tscpaths -p tsconfig.build.json -s ./src -o dist/lib && rollup -c && rm -f tsconfig.tsbuildinfo",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
     "test": "jest --passWithNoTests",
  "clear": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "test:cov": "jest --coverage --no-cache"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/dates": "^0.1.0-alpha3",
    "@lyvely/timers-interface": "^0.1.0-alpha3",
    "@lyvely/calendar-plan-interface": "^0.1.0-alpha3",
    "@lyvely/interface": "^0.1.0-alpha3",
    "@buddh4/mapped-types": "^1.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "0.1.13"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
