{
  "name": "@lyvely/deprecated-server",
  "type": "commonjs",
  "version": "0.1.0-alpha3",
  "description": "",
  "author": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build_disabled": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:e2e": "cross-env NODE_ENV=e2e npm run start",
    "start:e2e:debug": "cross-env NODE_ENV=e2e npm run start:debug",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test_disabled": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "cypress:open": "cypress open",
    "migrate:create": "sh -c 'ts-node node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate:up": "ts-node node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts",
    "deps": "npx depcruise --config .dependency-cruiser.js src"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@lyvely/core": "^0.1.0-alpha3",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bullmq": "^1.1.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/throttler": "^4.0.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^3.12.0",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.7",
    "eventemitter2": "^6.4.9",
    "express": "^4.18.2",
    "fastq": "^1.15.0",
    "file-type": "^18.3.0",
    "he": "^1.2.0",
    "helmet": "^6.1.5",
    "i18n": "^0.15.1",
    "joi": "^17.9.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "minimatch": "^9.0.0",
    "mongodb": "^4.13.0",
    "mongoose": "^7.1.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.2.6",
    "nestjs-pino": "^3.2.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^8.3.3",
    "pug": "^3.0.2",
    "randomcolor": "^0.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.12",
    "@types/minimatch": "^5.1.2",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.5",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "migrate-mongo": "^10.0.0",
    "mongo-seeding": "^3.7.2",
    "mongodb-memory-server": "^8.12.2",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.4"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "5.0.9"
    }
  }
}
