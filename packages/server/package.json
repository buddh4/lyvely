{
  "name": "lyvely-server",
  "type": "commonjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:e2e": "cross-env NODE_ENV=e2e npm run start",
    "start:e2e:debug": "cross-env NODE_ENV=e2e npm run start:debug",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "cypress:open": "cypress open",
    "migrate:create": "sh -c 'ts-node node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate:up": "ts-node node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/serve-static": "^2.2.2",
    "@types/minimatch": "^3.0.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.10.7",
    "joi": "^17.5.0",
    "jwt-decode": "^3.1.2",
    "minimatch": "^3.0.4",
    "mongodb": "^4.2.1",
    "mongoose": "^6.1.2",
    "ms": "^2.1.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^14.17.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "migrate-mongo": "^8.2.3",
    "mongo-seeding": "^3.7.0",
    "mongodb-memory-server": "^8.3.0",
    "prettier": "^2.1.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": ["js", "json", "ts"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },

    "modulePathIgnorePatterns": ["cypress"],
    "collectCoverageFrom": ["src/**/*.ts"],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/(?!lyvely-common/.*)"
    ]
  }
}
