{
  "name": "@lyvely/server",
  "type": "commonjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:e2e": "cross-env NODE_ENV=e2e npm run start",
    "start:e2e:debug": "cross-env NODE_ENV=e2e npm run start:debug",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "cypress:open": "cypress open",
    "migrate:create": "sh -c 'ts-node node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate:up": "ts-node node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts",
    "deps": "npx depcruise --config .dependency-cruiser.js src"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.6",
    "@automapper/core": "^8.7.6",
    "@automapper/nestjs": "^8.7.6",
    "@lyvely/common": "^0.0.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bullmq": "^1.0.0",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.6",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/throttler": "^3.1.0",
    "@types/minimatch": "^3.0.5",
    "bcrypt": "^5.0.1",
    "bullmq": "^3.2.2",
    "canvas": "^2.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.3",
    "express": "*",
    "file-type": "^18.0.0",
    "he": "^1.2.0",
    "helmet": "^6.0.0",
    "i18n": "^0.15.1",
    "joi": "^17.5.0",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.4",
    "mongodb": "^4.9.1",
    "mongoose": "^6.6.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "randomcolor": "^0.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "sharp": "^0.31.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.17.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^28.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^9.3.1",
    "dotenv": "^8.2.0",
    "jest": "^28.1.2",
    "migrate-mongo": "^8.2.3",
    "mongo-seeding": "^3.7.0",
    "mongodb-memory-server": "^8.3.0",
    "prettier": "^2.1.2",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.3"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/(?!@lyvely/common/.*)"
    ]
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "5.0.9"
    }
  }
}
