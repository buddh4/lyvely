{
  "name": "@lyvely/web",
  "author": "Julian Harrer",
  "version": "0.1.0-alpha3",
  "description": "core-web package",
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./tailwind.txt": {
      "import": "./dist/tailwind.txt",
      "require": "./dist/tailwind.txt"
    }
  },
  "scripts": {
    "test": "vitest --run",
    "cypress:open": "cross-env NODE_ENV=e2e cypress open",
    "cypress:run": "cross-env NODE_ENV=e2e cypress run",
    "build": "lyvely-dev build:web",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix",
    "format": "prettier .  --write",
    "prettier": "prettier . --check",
    "dev": "vite",
    "dev:e2e": "cross-env NODE_ENV=e2e pnpm run dev",
    "deps": "npx depcruise --config .dependency-cruiser.js src"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.5.0",
    "@intlify/core-base": "9.5.0",
    "@lyvely/common": "workspace:*",
    "@lyvely/dates": "workspace:*",
    "@lyvely/interface": "workspace:*",
    "@lyvely/ui": "workspace:*",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.9.0",
    "animate.css": "^4.1.1",
    "axios": "^1.6.8",
    "axios-auth-refresh": "^3.3.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cropperjs": "^1.5.13",
    "echarts": "^5.4.2",
    "intro.js": "^7.2.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "reflect-metadata": "0.1.13",
    "vue": "^3.4.25",
    "vue-i18n": "^9.5.0",
    "vue-randomcolor": "^1.0.4",
    "vue-router": "^4.3.2",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "tailwindcss": "^3.4.3",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@lyvely/devtools": "workspace:*",
    "@swc/core": "^1.3.93",
    "@types/js-cookie": "^3.0.6",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "cypress": "^13.6.0",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "@types/ms": "^0.7.34",
    "uuid": "^8.3.2",
    "vitest": "^1.5.1",
    "tslib": "^2.6.2",
    "@vue/runtime-dom": "^3.4.25",
    "postcss-import": "^16.1.0",
    "autoprefixer": "^10.4.19",
    "vue-tsc": "^2.0.19",
    "@types/lodash": "^4.17.4",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.0"
  }
}
