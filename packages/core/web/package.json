{
  "name": "@lyvely/web",
  "author": "Julian Harrer",
  "version": "0.1.0-alpha3",
  "description": "core-web package",
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./tailwind.txt": {
      "import": "./dist/tailwind.txt",
      "require": "./dist/tailwind.txt"
    }
  },
  "scripts": {
    "test": "vitest --run",
    "cypress:open": "cross-env NODE_ENV=e2e cypress open",
    "cypress:run": "cross-env NODE_ENV=e2e cypress run",
    "build": "lyvely-dev build:web",
    "lint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier src --write",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "dev": "vite",
    "dev:e2e": "cross-env NODE_ENV=e2e pnpm run dev",
    "deps": "npx depcruise --config .dependency-cruiser.js src"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.5",
    "@intlify/core-base": "9.5.0",
    "@lyvely/common": "workspace:*",
    "@lyvely/dates": "workspace:*",
    "@lyvely/interface": "workspace:*",
    "@lyvely/ui": "workspace:*",
    "@vueuse/core": "^10.9",
    "animate.css": "^4.1.1",
    "axios": "^1.6.8",
    "axios-auth-refresh": "^3.3.6",
    "class-validator": "^0.14.0",
    "cropperjs": "^1.5.13",
    "echarts": "^5.4.2",
    "intro.js": "^7.2.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17",
    "ms": "^2.1.3",
    "mitt": "^3.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "reflect-metadata": "0.1.13",
    "vue": "^3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.3",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@lyvely/devtools": "workspace:*",
    "typescript": "^5.4",
    "tslib": "^2.6.2",
    "vite": "^5.2",
    "vitest": "^1.5",
    "@vitejs/plugin-vue": "^5.0",
    "@vue/runtime-core": "^3.4",
    "@vue/runtime-dom": "^3.4",
    "vue-tsc": "^2.0",
    "@intlify/unplugin-vue-i18n": "^0.10",
    "tailwindcss": "^3.4",
    "@tailwindcss/typography": "^0.5",
    "@types/js-cookie": "^3.0.6",
    "@types/nprogress": "^0.2.3",
    "@types/node": "^20.12",
    "postcss": "^8.4",
    "postcss-cli": "^11.0",
    "autoprefixer": "^10.4",
    "postcss-import": "^16.1",
    "cypress": "^13.6",
    "jsdom": "^23.0",
    "@types/lodash": "^4.17",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "eslint-plugin-vue": "^9.25",
    "prettier-plugin-tailwindcss": "^0.5",
    "vue-eslint-parser": "^9.4",
    "depcheck": "^1.4"
  }
}
