{
  "name": "@lyvely/web",
  "author": "Julian Harrer",
  "version": "0.1.0-alpha3",
  "description": "core-web package",
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./tailwind.txt": {
      "import": "./dist/tailwind.txt",
      "require": "./dist/tailwind.txt"
    }
  },
  "web-types": "../../../node_modules/@lyvely/ui/dist/web-types.json",
  "scripts": {
    "test": "vitest",
    "cypress:open": "cross-env NODE_ENV=e2e cypress open",
    "cypress:run": "cross-env NODE_ENV=e2e cypress run",
    "build": "lyvely-dev build:web",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix",
    "format": "prettier .  --write",
    "dev": "vite",
    "dev:e2e": "cross-env NODE_ENV=e2e npm run dev",
    "deps": "npx depcruise --config .dependency-cruiser.js src"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(ts|js|tsx|jsx)$": "@swc/jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^virtual:pwa-register/vue$": "<rootDir>/__mocks__/virtual:pwa-register/vue.cjs"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx|vue)$",
      "^.+\\.module\\.(css|sass|scss)$",
      "virtual:pwa-register/vue"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx",
      "vue",
      "node"
    ]
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.5.0",
    "@intlify/core-base": "9.5.0",
    "@lyvely/calendar-plan-interface": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/dates": "^0.1.0-alpha3",
    "@lyvely/interface": "^0.1.0-alpha3",
    "@lyvely/time-series-interface": "^0.1.0-alpha3",
    "@lyvely/timers-interface": "^0.1.0-alpha3",
    "@lyvely/ui": "^0.1.0-alpha3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.9.0",
    "animate.css": "^4.1.1",
    "axios": "^1.4.0",
    "axios-auth-refresh": "^3.3.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cropperjs": "^1.5.13",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "focus-trap": "^7.4.0",
    "intro.js": "^7.2.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "randomcolor": "^0.6.2",
    "reflect-metadata": "^0.1.13",
    "rehype-external-links": "^3.0.0",
    "slugify": "^1.6.6",
    "vue": "^3.4.25",
    "vue-i18n": "^9.5.0",
    "vue-randomcolor": "^1.0.4",
    "vue-router": "^4.3.2",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuedraggable": "^4.1.0",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@lyvely/devtools": "^0.1.0-alpha3",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/vue3-jest": "^29.2.6",
    "cypress": "^13.6.0",
    "jsonwebtoken": "^9.0.2",
    "jsdom": "^22.1.0",
    "ms": "^2.1.3",
    "uuid": "^8.3.2",
    "vitest": "^1.5.1"
  }
}
