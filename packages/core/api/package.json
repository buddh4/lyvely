{
  "name": "@lyvely/api",
  "version": "0.1.0-alpha3",
  "description": "Lyvely server core",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && gulp copyAssets",
    "dev": "cross-env NODE_ENV=development nest start",
    "dev:debug": "cross-env NODE_ENV=development  nest start --debug --watch",
    "dev:e2e": "cross-env NODE_ENV=e2e nest start",
    "dev:e2e:debug": "cross-env NODE_ENV=e2e nest start --debug --watch",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.{ts,vue}\"",
    "lint:fix": "eslint -c .eslintrc.cjs \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "deps": "npx depcruise --config .dependency-cruiser.js src",
    "test": "jest --passWithNoTests",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "migrate:create": "sh -c 'ts-node ../../../node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node../../../ node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node ../../../node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate:up": "ts-node ../../../node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/dates": "^0.1.0-alpha3",
    "@lyvely/interface": "^0.1.0-alpha3",
    "@nestjs-modules/mailer": "~1.10.0",
    "@nestjs/bullmq": "10.1.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/testing": "^10.3.8",
    "@nestjs/throttler": "^5.1.2",
    "nestjs-i18n": "10.4.5",
    "nodemailer": "^6.9.1",
    "bcrypt": "^5.1.0",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "4.19.2",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "reflect-metadata": "0.1.13",
    "ms": "^2.1.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minimatch": "^9.0.0",
    "mongodb": "6.5.0",
    "mongoose": "8.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "randomcolor": "^0.6.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/multer": "^1.4.11",
    "@types/pug": "2.0.6",
    "del": "^6.1.1",
    "migrate-mongo": "^11.0.0",
    "rimraf": "^5.0.5"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
