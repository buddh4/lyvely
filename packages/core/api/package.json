{
  "name": "@lyvely/api",
  "author": "Julian Harrer",
  "license": "AGPL-3.0-or-later",
  "version": "0.1.1",
  "description": "Lyvely server core",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && gulp copyAssets",
    "test": "jest --silent",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.ts\" --fix",
    "format": "prettier src --write",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "ncu": "ncu",
    "deps": "rush-pnpm --rush-skip-checks depcruise --include-only \"^src\" --config .dependency-cruiser.js src",
    "dev": "cross-env NODE_ENV=development nest start",
    "dev:debug": "cross-env NODE_ENV=development  nest start --debug --watch",
    "dev:e2e": "cross-env NODE_ENV=e2e nest start",
    "dev:e2e:debug": "cross-env NODE_ENV=e2e nest start --debug --watch",
    "prepublish:npm": "pnpm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "pnpm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@lyvely/common": "workspace:*",
    "@lyvely/dates": "workspace:*",
    "@lyvely/interface": "workspace:*",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/event-emitter": "^2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/testing": "^10.3.8",
    "@nestjs/throttler": "^5.1.2",
    "bcrypt": "^5.1.0",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "express": "4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0",
    "mongoose": "~8.4.0",
    "mongodb": "6.6.2",
    "ms": "^2.1.3",
    "nestjs-i18n": "10.4.5",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "sharp": "^0.33.4",
    "randomcolor": "^0.6",
    "reflect-metadata": "^0.1.12 || ^0.2.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6",
    "file-type": "^19.0.0",
    "uuid": "^9.0",
    "mongo-seeding": "^4.0",
    "rimraf": "^5.0",
    "mongodb-memory-server": "^8.13.0"
  },
  "devDependencies": {
    "typescript": "^5.4",
    "@lyvely/devtools": "workspace:*",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/pug": "2.0.6",
    "@types/express-serve-static-core": "^4.19.0",
    "del": "^6.1.1",
    "gulp": "^5.0.0",
    "@types/qs": "^6.9.15",
    "eventemitter2": "^6.4.9",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.4",
    "tsc-alias": "^1.8.10",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "depcheck": "^1.4",
    "dependency-cruiser": "^14.1",
    "bullmq": "5.1.11",
    "why-is-node-running": "^2.2.2",
    "cross-env": "^7.0"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
