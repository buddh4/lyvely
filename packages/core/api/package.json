{
  "name": "@lyvely/api",
  "version": "0.1.0-alpha3",
  "description": "Lyvely server core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && gulp copyAssets",
    "dev": "cross-env NODE_ENV=development nest start",
    "dev:debug": "cross-env NODE_ENV=development  nest start --debug --watch",
    "dev:e2e": "cross-env NODE_ENV=e2e nest start",
    "dev:e2e:debug": "cross-env NODE_ENV=e2e nest start --debug --watch",
    "lint": "eslint --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "deps": "npx depcruise --config .dependency-cruiser.js src",
    "test": "jest --passWithNoTests",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "migrate:create": "sh -c 'ts-node ../../../node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node../../../ node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node ../../../node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate:up": "ts-node ../../../node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/testing": "^9.4.0",
    "@nestjs/throttler": "^4.0.0",
    "@lyvely/dates": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/interface": "^0.1.0-alpha3",
    "express": "^4.18.2",
    "mongoose": "^7.1.0",
    "mongodb": "^4.13.0",
    "class-transformer": "^0.5.1",
    "helmet": "^6.1.5",
    "lodash": "^4.17.21",
    "eventemitter2": "^6.4.9",
    "nodemailer": "^6.9.1",
    "reflect-metadata": "0.1.13",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "ms": "^2.1.3",
    "uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "canvas": "^2.11.2",
    "class-validator": "^0.14.0",
    "rxjs": "^7.8.1",
    "nestjs-i18n": "^10.3.7",
    "pug": "^3.0.2",
    "bullmq": "^3.12.0",
    "jsonwebtoken": "^9.0.0",
    "minimatch": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "randomcolor": "^0.6.2",
    "@nestjs/bullmq": "^1.1.0"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "migrate-mongo": "^10.0.0",
    "@types/pug": "2.0.6",
    "del": "^6.1.1"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
