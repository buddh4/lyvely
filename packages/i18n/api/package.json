{
  "name": "@lyvely/i18n",
  "version": "0.1.0-alpha3",
  "description": "Lyvely i18n module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "test": "jest --passWithNoTests --forceExit"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@lyvely/users": "^0.1.0-alpha3",
    "@nestjs/event-emitter": "^1.4.1",
    "rxjs": "^7.8.1",
    "lodash": "^4.17.21",
    "nestjs-i18n": "^10.2.6",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@lyvely/core": "^0.1.0-alpha3",
    "@lyvely/testing": "^0.1.0-alpha3",
    "@nestjs/testing": "^9.4.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@lyvely/core": "^0.1.0-alpha3",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "express": "^4.18.2",
    "mongoose": " ^7.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
