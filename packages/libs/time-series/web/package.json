{
  "name": "@lyvely/time-series-web",
  "version": "0.1.0-alpha3",
  "description": "time-series-web package",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./tailwind.txt": {
      "import": "./dist/tailwind.txt",
      "require": "./dist/tailwind.txt"
    }
  },
  "scripts": {
    "build": "lyvely-dev build:web",
    "build:staging": "lyvely-dev build:web:staging",
    "build:dev": "lyvely-dev build:web:development",
    "lint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "deps": "pnpm depcruise --include-only \"^src\" --config .dependency-cruiser.js src",
    "ncu": "ncu",
    "format": "prettier .  --write"
  },
  "author": "Julian Harrer",
  "dependencies": {
    "vue": "^3.4",
    "@vueuse/core": "^10.9",
    "pinia": "^2.0",
    "@lyvely/interface": "workspace:*",
    "@lyvely/time-series-interface": "workspace:*",
    "@lyvely/ui": "workspace:*",
    "@lyvely/web": "workspace:*",
    "@lyvely/calendar-plan-web": "workspace:*",
    "@lyvely/dates": "workspace:*",
    "@lyvely/timers-interface": "workspace:*",
    "echarts": "^5.5",
    "class-validator": "^0.14.0"
  },
  "devDependencies": {
    "@lyvely/devtools": "workspace:*",
    "vite-plugin-externalize-deps": "^0.8",
    "vite": "^5.2",
    "vite-tsconfig-paths": "^4.3",
    "vue-tsc": "^2.0",
    "@vue/runtime-dom": "^3.4",
    "vitest": "^1.5",
    "typescript": "^5.4",
    "tslib": "^2.6.2",
    "postcss": "^8.4",
    "postcss-cli": "^11.0",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "eslint-plugin-vue": "^9.25",
    "prettier-plugin-tailwindcss": "^0.5",
    "vue-eslint-parser": "^9.4",
    "depcheck": "^1.4",
    "dependency-cruiser": "^14.1"
  }
}
