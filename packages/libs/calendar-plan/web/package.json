{
  "name": "@lyvely/calendar-plan-web",
  "version": "0.1.0-alpha3",
  "description": "calendar-plan-web package",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./tailwind.txt": {
      "import": "./dist/tailwind.txt",
      "require": "./dist/tailwind.txt"
    }
  },
  "scripts": {
    "build": "lyvely-dev build:web",
    "build:staging": "vite build --mode staging && vue-tsc --emitDeclarationOnly",
    "build:dev": "vite build --mode development && vue-tsc --emitDeclarationOnly",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix",
    "prettier": "prettier . --check",
    "format": "prettier .  --write"
  },
  "author": "Julian Harrer",
  "dependencies": {
    "vue": "^3.4.25",
    "@vueuse/core": "^10.9.0",
    "pinia": "^2.0.35",
    "@lyvely/interface": "^0.1.0-alpha3",
    "@lyvely/calendar-plan-interface": "^0.1.0-alpha3",
    "@lyvely/ui": "^0.1.0-alpha3",
    "@lyvely/web": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/dates": "^0.1.0-alpha3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@lyvely/devtools": "^0.1.0-alpha3",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.2",
    "vue-tsc": "^2.0.14",
    "@vue/runtime-dom": "^3.4.25"
  }
}
