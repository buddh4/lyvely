{
  "name": "@lyvely/ui",
  "version": "0.1.0",
  "author": "Julian Harrer",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "module": "./dist/lyvely-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lyvely-ui.js",
      "require": "./dist/lyvely-ui.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./tailwind.txt": {
      "import": "./dist/tailwind.txt",
      "require": "./dist/tailwind.txt"
    }
  },
  "web-types": "./dist/web-types.json",
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier src --write",
    "prettier": "prettier src --check",
    "depcheck": "depcheck --quiet",
    "ncu": "ncu",
    "test": "vitest run --silent",
    "dev": "vite",
    "build": "lyvely-dev build:web && node bin/generate-types.js",
    "build:types": "node bin/generate-types.js",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deps": "rush-pnpm --rush-skip-checks depcruise --config .dependency-cruiser.cjs src"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@lyvely/common": "workspace:*",
    "@lyvely/dates": "workspace:*",
    "vue": "^3.4",
    "vue-router": "^4.3",
    "@ckpack/vue-color": "^1.5",
    "@vueuse/core": "^10.9",
    "lodash": "^4.17",
    "randomcolor": "^0.6",
    "slugify": "^1.6",
    "tailwind-merge": "^2.3.0",
    "markdown-it": "^14.1.0"
  },
  "devDependencies": {
    "@lyvely/devtools": "workspace:*",
    "@tailwindcss/typography": "^0.5",
    "@types/randomcolor": "^0.5",
    "@types/zxcvbn": "^4.4",
    "@vitejs/plugin-vue": "^5.0",
    "@vue/test-utils": "^2.4",
    "@types/markdown-it": "^14.1.1",
    "glob": "^10.3",
    "jsdom": "^23.0",
    "postcss": "^8.4",
    "postcss-cli": "^11.0",
    "postcss-import": "^16.1",
    "tailwindcss": "^3.4",
    "typescript": "^5.4",
    "vite": "^5.2",
    "vite-plugin-dts": "^3.9",
    "vite-plugin-externalize-deps": "^0.8",
    "vite-tsconfig-paths": "^4.3",
    "vitest": "^1.5",
    "vue-docgen-api": "^4.74",
    "vue-tsc": "^2.0",
    "@types/lodash": "^4.17",
    "@types/node": "^20.12",
    "sass": "^1.77",
    "storybook": "^7.5",
    "storybook-tailwind-dark-mode": "^1.0",
    "@storybook/addon-essentials": "^7.5",
    "@storybook/addon-interactions": "^7.5",
    "@storybook/addon-links": "^7.5",
    "@storybook/addon-themes": "^8.1",
    "@storybook/addon-controls": "^7.5.1",
    "@storybook/blocks": "^7.5",
    "@storybook/api": "^7.6",
    "@storybook/testing-library": "^0.2",
    "@storybook/vue3": "^8.0",
    "@storybook/vue3-vite": "^8.0",
    "@storybook/addons": "^7.6",
    "react": "^18.2",
    "react-dom": "^18.2",
    "depcheck": "^1.4",
    "dependency-cruiser": "^14.1",
    "eslint": "^8.24",
    "@typescript-eslint/eslint-plugin": "^7.9",
    "@typescript-eslint/parser": "^7.9",
    "eslint-config-prettier": "^8.8",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^5.1",
    "eslint-plugin-vue": "^9.25",
    "prettier-plugin-tailwindcss": "^0.5",
    "vue-eslint-parser": "^9.4",
    "@ctrl/tinycolor": "^3.6"
  }
}
