{
  "name": "@lyvely/timers",
  "version": "0.1.0-alpha3",
  "description": "Lyvely timers module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf ./dist/* && tsc -p tsconfig.build.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@nestjs/mongoose": "^9.2.2",
    "reflect-metadata": "0.1.13",
    "@lyvely/core": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/timers-interface": "^0.1.0-alpha3",
    "@lyvely/users": "^0.1.0-alpha3",
    "mongoose": "^6.0.2 || ^7.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.4.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "compilerOptions": {
            "sourceMap": true,
            "inlineSourceMap": true
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
