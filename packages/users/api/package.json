{
  "name": "@lyvely/users",
  "version": "0.1.0-alpha3",
  "description": "Lyvely user api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "test": "jest"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@lyvely/core": "0.1.0-alpha3",
    "@lyvely/users-interface": "0.1.0-alpha3",
    "@lyvely/dates": "0.1.0-alpha3",
    "@lyvely/testing": "0.1.0-alpha3",
    "@lyvely/policies": "0.1.0-alpha3",
    "@lyvely/throttler": "0.1.0-alpha3",
    "@lyvely/profiles-interface": "0.1.0-alpha3",
    "reflect-metadata": "0.1.13",
    "mongoose": "^7.1.0",
    "bcrypt": "^5.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.4.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
