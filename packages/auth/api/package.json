{
  "name": "@lyvely/auth",
  "version": "0.1.0-alpha3",
  "description": "Lyvely policy module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "test": "jest --passWithNoTests --forceExit"
  },
  "author": "Julian Harrer",
  "license": "AGPL-3.0",
  "dependencies": {
    "@nestjs/core": "^9.4.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@lyvely/core": "^0.1.0-alpha3",
    "@lyvely/common": "^0.1.0-alpha3",
    "@lyvely/captchas": "^0.1.0-alpha3",
    "@lyvely/mails": "^0.1.0-alpha3",
    "@lyvely/jwt": "^0.1.0-alpha3",
    "@lyvely/throttler": "^0.1.0-alpha3",
    "@lyvely/users": "^0.1.0-alpha3",
    "@lyvely/dates": "^0.1.0-alpha3",
    "@lyvely/auth-interface": "^0.1.0-alpha3",
    "ms": "^2.1.3",
    "uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@lyvely/testing": "^0.1.0-alpha3",
    "@nestjs/testing": "^9.4.0",
    "reflect-metadata": "0.1.13",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@lyvely/core": "^0.1.0-alpha3",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "express": "^4.18.2",
    "mongoose": " ^7.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "jest": {
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testRegex": "[^cypress]*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
