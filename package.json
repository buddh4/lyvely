{
  "name": "lyvely-app",
  "version": "0.1.0-alpha3",
  "author": "Julian Harrer",
  "private": true,
  "workspaces": [
    "packages/docs",
    "packages/core/api",
    "packages/core/interface",
    "packages/core/web",
    "packages/libs/common",
    "packages/libs/calendar-plan/api",
    "packages/libs/calendar-plan/interface",
    "packages/libs/calendar-plan/web",
    "packages/libs/dates",
    "packages/libs/testing",
    "packages/libs/time-series/api",
    "packages/libs/time-series/interface",
    "packages/libs/time-series/web",
    "packages/libs/timers/api",
    "packages/libs/timers/interface",
    "packages/libs/ui",
    "packages/libs/devtools",
    "packages/applications/pwa",
    "packages/applications/server",
    "packages/features/activities/web",
    "packages/features/habits/api",
    "packages/features/habits/interface",
    "packages/features/habits/web",
    "packages/features/journals/api",
    "packages/features/journals/interface",
    "packages/features/journals/web",
    "packages/features/legal/api",
    "packages/features/legal/interface",
    "packages/features/legal/web",
    "packages/features/milestones/api",
    "packages/features/milestones/interface",
    "packages/features/milestones/web",
    "packages/features/analytics/api",
    "packages/features/analytics/interface",
    "packages/features/analytics/web",
    "packages/features/tasks/api",
    "packages/features/tasks/interface",
    "packages/features/tasks/web"
  ],
  "scripts": {
    "docker:build:static": "docker build -t lyvely-static -f docker/Dockerfile.api .",
    "lint": "eslint -c .eslintrc.js \"packages/*/src/**/*.{ts,vue}\"",
    "lint:fix": "eslint -c .eslintrc.js \"packages/*/src/**/*.{ts,vue}\" --fix",
    "test": "npm run test -w @lyvely/common && npm run test -w @lyvely/server",
    "core:build": "npm run build -w @lyvely/interface && concurrently \"npm run build -w @lyvely/web\" \"npm run build -w @lyvely/api\"",
    "ui:build": "npm run build -w @lyvely/ui",
    "timers:timers": "npm run build -w @lyvely/timers-interface && npm run build -w @lyvely/timers",
    "time-series:build": "npm run build -w @lyvely/time-series-interface && concurrently \"npm run build -w @lyvely/time-series-web\" \"npm run build -w @lyvely/time-series\"",
    "calendar-plan:build": "npm run build -w @lyvely/calendar-plan-interface && concurrently \"npm run build -w @lyvely/calendar-plan-web\" \"npm run build -w @lyvely/calendar-plan\"",
    "testing:build": "npm run build -w @lyvely/testing",
    "dates:build": "npm run build -w @lyvely/dates",
    "common:build": "npm run build -w @lyvely/common",
    "tasks:build": "npm run build -w @lyvely/tasks-interface && concurrently \"npm run build -w @lyvely/tasks\" \"npm run build -w @lyvely/tasks-web\"",
    "habits:build": "npm run build -w @lyvely/habits-interface && concurrently \"npm run build -w @lyvely/habits\" \"npm run build -w @lyvely/habits-web\"",
    "milestones:build": "npm run build -w @lyvely/milestones-interface && concurrently \"npm run build -w @lyvely/milestones\" \"npm run build -w @lyvely/milestones-web\"",
    "activities:build": "concurrently \"npm run build -w @lyvely/activities-web\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nx/devkit": "^16.3.2",
    "@nx/eslint-plugin": "^16.3.2",
    "eslint-config-prettier": "^8.8.0",
    "npm-watch": "^0.11.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/testing": "^9.4.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vue/test-utils": "^2.4.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "dependency-cruiser": "^14.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "mini-svg-data-uri": "^1.4.4",
    "mongo-seeding": "^3.7.2",
    "mongodb-memory-server": "^8.13.0",
    "ncp": "^2.0.0",
    "nx": "16.3.2",
    "nx-cloud": "latest",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "2.7.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "15.11.0",
    "rollup": "^3.21.3",
    "rollup-plugin-node-externals": "^6.1.2",
    "rollup-plugin-vue": "^6.0.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-externalize-deps": "^0.7.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^1.0.1",
    "vue-tsc": "^1.6.5"
  }
}
