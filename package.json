{
  "name": "lyvely-app",
  "version": "0.1.0-alpha3",
  "author": "Julian Harrer",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "docker:build:static": "docker build -t lyvely-static -f docker/Dockerfile.api .",
    "lint": "pnpm nx run-many lint",
    "format": "pnpm nx run-many format",
    "server:build": "pnpm -F @lyvely/server build",
    "server:test": "pnpm -F @lyvely/server test",
    "server:dev": "pnpm -F @lyvely/server start:dev",
    "server:debug": "pnpm -F @lyvely/server start:debug",
    "server:prod": "pnpm -F @lyvely/server start:prod",
    "web:build": "pnpm -F @lyvely/web build",
    "web:dev": "pnpm -F @lyvely/web dev",
    "core:build": "pnpm -F @lyvely/interface build && concurrently \"pnpm -F @lyvely/api build\" \"pnpm -F @lyvely/web build\"",
    "ui:build": "pnpm -F @lyvely/ui build"
  },
  "dependencies": {
    "sharp": "^0.33.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/testing": "^10.3.8",
    "nx": "^19.0.4",
    "@nx/devkit": "^19.0.4",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vue/test-utils": "^2.4.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "dependency-cruiser": "^14.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "mini-svg-data-uri": "^1.4.4",
    "mongo-seeding": "^4.0.0",
    "mongodb-memory-server": "^8.13.0",
    "ncp": "^2.0.0",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "reflect-metadata": "0.1.13",
    "release-it": "^17.2.1",
    "rollup": "^3.21.3",
    "rollup-plugin-node-externals": "^6.1.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.77.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2",
    "vite": "^5.2.10",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.1",
    "vue-tsc": "^2.0.14"
  }
}
