{
  "name": "lyvely-app",
  "version": "0.1.0-alpha3",
  "author": "Julian Harrer",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/**/*"
  ],
  "scripts": {
    "docker:build:static": "docker build -t lyvely-static -f docker/Dockerfile.api .",
    "lint": "eslint -c .eslintrc.js \"packages/*/src/**/*.{ts,vue}\"",
    "lint:fix": "eslint -c .eslintrc.js \"packages/*/src/**/*.{ts,vue}\" --fix",
    "common:watch": "npm run watch -w @lyvely/common",
    "common:build": "npm run build -w @lyvely/common",
    "common:deps": "npm run deps -w @lyvely/common",
    "common:test": "npm run test -w @lyvely/common",
    "test": "npm run test -w @lyvely/common && npm run test -w @lyvely/server",
    "server:build": "npm run build -w @lyvely/server",
    "server:test": "npm run test -w @lyvely/server",
    "server:dev": "npm run start:dev -w @lyvely/server",
    "server:debug": "npm run start:debug -w @lyvely/server",
    "server:deps": "npm run deps -w @lyvely/server",
    "server:prod": "npm run start:prod -w @lyvely/server",
    "web:build": "npm run build -w @lyvely/web",
    "web:dev": "npm run dev -w @lyvely/web",
    "web:deps": "npm run deps -w @lyvely/web",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nx/devkit": "^16.3.2",
    "@nx/eslint-plugin": "^16.3.2",
    "eslint-config-prettier": "^8.8.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "dependency-cruiser": "^12.12.0",
    "eslint": "^8.24.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.1",
    "nx": "16.3.2",
    "nx-cloud": "latest",
    "prettier": "2.7.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1"
  }
}
